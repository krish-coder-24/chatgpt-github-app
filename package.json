{
  "name": "vitalai-platform",
  "version": "1.0.0",
  "description": "VitalAI - Your Personal Medical Professional Platform",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/app.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "setup": "npm run install:all && cp .env.example .env",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["medical", "ai", "health", "platform", "baymax"],
  "author": "VitalAI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "socket.io": "^4.7.4",
    "morgan": "^1.10.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}